	volume = {29},
	issn = {1543-5938, 1545-2050},
	shorttitle = {{MARINE} {RESERVES} {AND} {OCEAN} {NEIGHBORHOODS}},
	url = {https://www.annualreviews.org/content/journals/10.1146/annurev.energy.29.062403.102254},
